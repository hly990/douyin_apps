{
  "kind": "collectionType",
  "collectionName": "video_histories",
  "info": {
    "singularName": "video-history",
    "pluralName": "video-histories",
    "displayName": "视频观看历史",
    "description": "存储用户观看视频的历史记录"
  },
  "options": {
    "draftAndPublish": false,
    "comment": "记录用户观看视频的历史记录"
  },
  "attributes": {
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::user.user"
    },
    "video": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::video.video"
    },
    "watchTime": {
      "type": "datetime",
      "default": "now",
      "required": true
    },
    "progress": {
      "type": "decimal",
      "default": 0,
      "comment": "观看进度，0-1之间的值"
    }
  }
} 